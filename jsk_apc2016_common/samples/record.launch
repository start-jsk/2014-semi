<launch>
  <arg name="bagfile" />
  <node name="rosbag_record"
        pkg="rosbag" type="record"
        args="-O $(arg bagfile)
              -b 0
	      
	      /tf
	      /joint_states
	      /base_link
	      /cluster_indices_decomposer_target/debug_output
	      /head_camera/rgb/camera_info
	      /head_camera/rgb/image_raw
	      /head_camera/depth_registered/camera_info
	      /head_camera/depth_registered/points
	      /head_camera/depth_registered/image_raw
	      /cluster_indices_decomposer_target/centroid_pose_array
	      /cluster_indices_decomposer_target/boxes
	      /move_group/monitored_planning_scene
	      /multi_plane_estimate/output_polygon
	      "
        output="screen">
  </node>
</launch>
