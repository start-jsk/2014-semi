<launch>
  <node pkg="jsk_2016_01_baxter_apc" name="work_order" type="work_order.py" >
    <rosparam subst_value="True">
       json: $(find jsk_apc2016_common)/json/pick_layout_1.json
       rate: 10
       apc2016: true
       gripper: gripper2015
       max_weight: -1
    </rosparam>
  </node>

  <test test-name="work_order_published" pkg="jsk_tools"  type="test_topic_published.py" >
    <param name="~topic_0" value="/work_order/left_hand" />
    <param name="~timeout_0" value="10" />
    <param name="~topic_1" value="/work_order/right_hand" />
    <param name="~timeout_1" value="10" />
  </test>
</launch>
