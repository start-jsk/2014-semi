<?xml version="1.0"?>
<package format="3">
  <name>instance_occlsegm</name>
  <version>4.3.0</version>
  <description>Learning Instance Occlusion Segmentation.</description>

  <maintainer email="www.kentaro.wada@gmail.com">Kentaro Wada</maintainer>
  <author email="www.kentaro.wada@gmail.com">wkentaro</author>

  <license>MIT</license>
  <url type="repository">https://github.com/start-jsk/jsk_apc</url>
  <url type="bugtracker">https://github.com/start-jsk/jsk_apc/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>catkin_virtualenv</build_depend>

  <!-- For 'import matplotlib.pyplot' -->
  <exec_depend condition="$ROS_PYTHON_VERSION==2">python-tk</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION==3">python3-tk</exec_depend>
  <exec_depend>synthetic2d</exec_depend>

  <export>
    <pip_requirements>requirements.txt</pip_requirements>
  </export>
</package>
