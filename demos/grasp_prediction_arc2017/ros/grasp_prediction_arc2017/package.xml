<?xml version="1.0"?>
<package format="3">
  <name>grasp_prediction_arc2017</name>
  <version>4.3.0</version>
  <description>Grasp prediction for ARC2017.</description>

  <maintainer email="www.kentaro.wada@gmail.com">Kentaro Wada</maintainer>
  <author email="www.kentaro.wada@gmail.com">Kentaro Wada</author>

  <license>MIT</license>
  <url type="repository">https://github.com/start-jsk/jsk_apc</url>
  <url type="bugtracker">https://github.com/start-jsk/jsk_apc/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>catkin_virtualenv</build_depend>
  <build_depend>grasp_fusion</build_depend>
  <build_depend condition="$ROS_PYTHON_VERSION==2">python-gdown-pip</build_depend>
  <build_depend condition="$ROS_PYTHON_VERSION==3">python3-gdown-pip</build_depend>

  <exec_depend>grasp_fusion</exec_depend>
  <exec_depend>image_transport</exec_depend>
  <exec_depend>image_view</exec_depend>
  <exec_depend>jsk_arc2017_baxter</exec_depend>
  <exec_depend>jsk_perception</exec_depend>
  <exec_depend>nodelet</exec_depend>
  <exec_depend>openni2_launch</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION==2">python-chainer-pip</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION==3">python3-chainer-pip</exec_depend>
  <exec_depend>rqt_gui</exec_depend>
  <exec_depend>rosbag</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>sphand_driver</exec_depend>
  <exec_depend>stereo_image_proc</exec_depend>
  <exec_depend>tf</exec_depend>

  <test_depend>jsk_apc2016_common</test_depend>
  <test_depend>jsk_tools</test_depend>
  <test_depend>roslint</test_depend>
  <test_depend>rostest</test_depend>

  <export>
  </export>
</package>
